<h1>組織を選択</h1>

<%= form_with model: current_user, url: organization_setting_path, local: true, method: :patch do |f| %>
  <div>
    <%= f.label :organization_id, '組織' %><br>
    <%= f.collection_select :organization_id, Organization.all, :id, :name, {prompt: '選択してください'} %>
  </div>
  <div>
    <%= f.submit '設定' %>
  </div>
<% end %>
